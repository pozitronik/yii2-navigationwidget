{
	"name": "pozitronik/yii2-navigationwidget",
	"description": "Dynamic navigation for YII2",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"extension",
		"widget",
		"navigation"
	],
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"pozitronik/yii2-helpers": "1.0.5",
		"pozitronik/yii2-cachedwidget": "~1.1.0",
		"yiisoft/yii2-bootstrap": "~2.0.0"
	},
	"require-dev": {
		"yiisoft/yii2": "~2.0.0"

	},
	"autoload": {
		"psr-4": {
			"pozitronik\\widgets\\": "src"
		}
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
